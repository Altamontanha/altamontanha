<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AltaMontanha" namespace="AltaMontanha.Models.Dominio">
	<class name="Categoria" table="tb_categoria" lazy="false">
		<id name="Codigo" access="property" column="CodCategoria" type="Int32">
			<generator class="identity" />
		</id>
		<property name="Titulo" not-null="true" access="property" type="String">
			<column name="Titulo" length="50" />
		</property>
		<property name="Descricao" not-null="true" access="property" type="String">
			<column name="Descricao" length="255" />
		</property>
	</class>
</hibernate-mapping>