<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="AltaMontanha" namespace="AltaMontanha.Models.Dominio" >
	<class name="Link" table="TB_Link" lazy="false">
		<id name="Codigo" access="property" column="CodLink" type="Int32">
			<generator class="identity" />
		</id>
		<property name="Titulo" not-null="true" access="property" type="String">
			<column name="Titulo" length="150" />
		</property>
		<property name="Url" not-null="true" access="property" type="String">
			<column name="Url" length="250" />
		</property>
	</class>
</hibernate-mapping>